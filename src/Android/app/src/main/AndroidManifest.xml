<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="bergmkyl.osu_pokedex">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".PostPokemonActivity" />
        <activity android:name=".PutPokemonActivity" />
        <activity android:name=".PostTrainerActivity" />
        <activity android:name=".PutTrainerActivity" />
        <activity android:name=".DeletePokemonActivity" />
        <activity android:name=".DeleteTrainerActivity" />
        <activity android:name=".AddCapturedPokemonActivity" />
        <activity android:name=".ReleasePokemonActivity"></activity>
    </application>

</manifest>